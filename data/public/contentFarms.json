{
    "redirect-rules": [
        {
            "src": "issuehint.com/issue/([^\\d]*)/",
            "kind": "regexp",
            "dst": "github.com/$1/issues/",
            "enable": true
        },
        {
            "src": "issuehint.com/repo/",
            "kind": "wildcard",
            "dst": "github.com/",
            "enable": true
        },
        {
            "src": "//qastack.*/superuser/",
            "kind": "wildcard",
            "dst": "//superuser.com/questions/",
            "enable": true
        },
        {
            "src": "//qastack.*/ubuntu/",
            "kind": "wildcard",
            "dst": "//askubuntu.com/questions/",
            "enable": true
        },
        {
            "src": "//qastack.*/server/",
            "kind": "wildcard",
            "dst": "//serverfault.com/questions/",
            "enable": true
        },
        {
            "src": "//qastack.*/programming/",
            "kind": "wildcard",
            "dst": "//stackoverflow.com/questions/",
            "enable": true
        },
        {
            "src": "\/\/qastack\\..{2,4}\/([^\/]*)",
            "kind": "regexp",
            "dst": "//$1.stackexchange.com/questions",
            "enable": true
        },
        {
            "src": "//*.softwareuser.asklobster.com/posts/",
            "kind": "wildcard",
            "dst": "//superuser.com/questions/",
            "enable": true
        },
        {
            "src": "\/\/.{2,3}\\.(\\w*)\\.asklobster\\.com\/posts\/",
            "kind": "regexp",
            "dst": "//$1.stackexchange.com/questions/",
            "enable": true
        },
        {
            "src": "\/\/.{2,3}\\.handyradar\\.org\/(\\d+)-.*",
            "kind": "regexp",
            "dst": "//askubuntu.com/questions/$1",
            "enable": true
        },
        {
            "src": "https?:\/\/www\\.it-swarm\\.cn\/cron\/.*\/(\\d+)\/",
            "kind": "regexp",
            "dst": "https://unix.stackexchange.com/questions/$1",
            "enable": true
        },
        {
            "src": "https?\/\/www\\.it-swarm\\.cn\/.*\/.*\/(\\d+)\/",
            "kind": "regexp",
            "dst": "https://stackoverflow.com/questions/$1",
            "enable": true
        },
        {
            "src": "https?://www.codenong.com/tag",
            "kind": "wildcard",
            "dst": "https://stackoverflow.com/tag",
            "enable": true
        },
        {
            "src": "https?:\/\/www\\.codenong\\.com\/(\\d+)",
            "kind": "regex",
            "dst": "https://stackoverflow.com/question/$1",
            "enable": true
        }
    ]
}
