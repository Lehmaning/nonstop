{
    "redirect-rules": [
        {
            "src": "//qastack.*/superuser/",
            "kind": "wildcard",
            "dst": "//superuser.com/questions/",
            "enable": true
        },
        {
            "src": "//qastack.*/ubuntu/",
            "kind": "wildcard",
            "dst": "//askubuntu.com/questions/",
            "enable": true
        },
        {
            "src": "//qastack.*/server/",
            "kind": "wildcard",
            "dst": "//serverfault.com/questions/",
            "enable": true
        },
        {
            "src": "//qastack.*/programming/",
            "kind": "wildcard",
            "dst": "//stackoverflow.com/questions/",
            "enable": true
        },
        {
            "src": "//qastack.*/*/",
            "kind": "wildcard",
            "dst": "//$2.stackexchange.com/questions/",
            "enable": true
        },
        {
            "src": "//*.softwareuser.asklobster.com/posts/",
            "kind": "wildcard",
            "dst": "//superuser.com/questions/",
            "enable": true
        },
        {
            "src": "\/\/.{2,3}\\.(\\w*)\\.asklobster\\.com\/posts\/",
            "kind": "regexp",
            "dst": "//$1.stackexchange.com/questions/",
            "enable": true
        },
        {
            "src": "\/\/.{2,3}\\.handyradar\\.org\/(\\d+)-.*",
            "kind": "regexp",
            "dst": "//askubuntu.com/questions/$1",
            "enable": true
        },
        {
            "src": "https?:\/\/www\\.it-swarm\\.cn\/cron\/.*\/(\\d+)\/",
            "kind": "regexp",
            "dst": "https://unix.stackexchange.com/questions/$1",
            "enable": true
        },
        {
            "src": "https?:\/\/www\\.it-swarm\\.cn\/.*\/.*\/(\\d+)\/",
            "kind": "regexp",
            "dst": "https://stackoverflow.com/questions/$1",
            "enable": true
        }
    ]
}
