{
    "redirect-rules": [
        {
            "src": "\/\/(\\w{2,3}\\.\\w{2,3}\/\\w{4, 8})",
            "kind": "regexp",
            "dst": "//unshort.link/$1",
            "enable": false
        },
        {
            "src": "//discord.gg",
            "kind": "wildcard",
            "dst": "//discord.com/invite",
            "enable": true
        },
        {
            "src": "\/\/youtu\\.be\/(.{11})",
            "kind": "regexp",
            "dst": "//www.youtube.com/watch?v=$1",
            "enable": true
        },
        {
            "src": "\/\/youtu\\.be\/",
            "kind": "regexp",
            "dst": "//invidious.snopyta.org/",
            "enable": false
        },
        {
            "src": "\/\/(?:acg\\.tv|b23\\.tv|b23\\.wtf|bili2233\\.cn)\/av",
            "kind": "regexp",
            "dst": "//www.bilibili.com/video/av",
            "enable": true
        },
        {
            "src": "//b23.tv/BV",
            "kind": "wildcard",
            "dst": "//www.bilibili.com/video/BV",
            "enable": true
        },
        {
            "src": "//b23.tv",
            "kind": "wildcard",
            "dst": "//b23.wtf",
            "enable": true
        },
        {
            "src": "//acg.tv/sm",
            "kind": "wildcard",
            "dst": "//www.nicovideo.jp/watch/sm",
            "enable": true
        },
        {
            "src": "\/\/(?:nico\\.ms|acg\\.tv)\/sm",
            "kind": "regexp",
            "dst": "//www.nicovideo.jp/watch/sm",
            "enable": true
        },
        {
            "src": "//nico.ms/im",
            "kind": "wildcard",
            "dst": "//seiga.nicovideo.jp/seiga/im",
            "enable": true
        }
    ]
}
